<participantAttributes>
	<attribute>
		<name>Postal Code</name>
		<assignableAtEnrollment>true</assignableAtEnrollment>
		<mandatoryAtEnrollment>true</mandatoryAtEnrollment>
		<mandatoryAtReception>true</mandatoryAtReception>
		<editableAtReception>true</editableAtReception>
		<editableAfterReception>false</editableAfterReception>
		<type>TEXT</type>
		<validators>
			<dataValidator dataType="INTEGER">
 				<rangeValidator> 
				<minimum>00000</minimum> 
				<maximum>99999</maximum> 
 				</rangeValidator> 
			</dataValidator>
		</validators>
	</attribute>
